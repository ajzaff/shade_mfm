void Init(Int x, Int y, Int w, Int h) {
	Atom S = new(Empty);

	if (random_oneIn(100)) {
		S = new(DReg); // dreg not ready yet
	} else if (random_oneIn(10)) {
		S = new(Res);
	}

	if (x == w/2 && y == h/2)
		S = new(FriendlyForkBomb);

	//if (x == w/4 && y == h/4)
	//	S = new(ForkBomb);

	ew(0, S); 
}